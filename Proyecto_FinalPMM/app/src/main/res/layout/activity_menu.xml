        android:layout_weight="1.5"></FrameLayout>

    <!--Fragment DinÃ¡mico-->
    <FrameLayout
        android:id="@+id/menuDinamicoID"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="3"
        tools:layout="@layout/fragment_menu_dinamico"></FrameLayout>

</LinearLayout>
